    <string name="terms_title">Terms &amp; Conditions</string>
    <string name="terms_last_updated">Last Updated: February 7, 2025</string>

    <string name="terms_intro">Welcome to Zero Hunger! These Terms &amp; Conditions govern your use of our app and services. By accessing or using the app, you agree to these terms.</string>

    <string name="terms_user_eligibility_title">1. User Eligibility</string>
    <string name="terms_user_eligibility">- You must be at least 18 years old to use this app.\n- If you are under 18, parental supervision is required.</string>

    <string name="terms_account_registration_title">2. Account Registration</string>
    <string name="terms_account_registration">- You must provide accurate information during signup.\n- You are responsible for maintaining your account security.</string>

    <string name="terms_donations_title">3. Donations &amp; Food Distribution</string>
    <string name="terms_donations">- Donations must comply with food safety standards.\n- Zero Hunger does not guarantee the quality or condition of donated food.</string>

    <string name="terms_prohibited_activities_title">4. Prohibited Activities</string>
    <string name="terms_prohibited_activities">You agree not to:\n- Misuse the app for fraudulent activities.\n- Post false or misleading information.\n- Violate any laws while using this service.</string>

    <string name="terms_privacy_policy_title">5. Privacy Policy</string>
    <string name="terms_privacy_policy">- We collect and store user data as per our Privacy Policy.\n- Your personal data will not be shared without consent.</string>

    <string name="terms_liability_disclaimer_title">6. Liability Disclaimer</string>
    <string name="terms_liability_disclaimer">- Zero Hunger is not liable for any food-related issues.\n- We do not assume responsibility for third-party interactions.</string>

    <string name="terms_account_termination_title">7. Termination of Account</string>
    <string name="terms_account_termination">- We may suspend or terminate your account for violating these terms.\n- You may delete your account at any time.</string>

    <string name="terms_changes_title">8. Changes to Terms</string>
    <string name="terms_changes">- We reserve the right to update these terms.\n- Changes will be notified via the app.</string>

    <string name="terms_contact_title">9. Contact Us</string>
    <string name="terms_contact">For any questions, reach out at support@zerohunger.com</string>

